<?xml version="1.0" encoding="utf-8"?>
<TableLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Interdictions.InterdictionsList"
    >
    <TextView
        android:id="@+id/txtInterdictions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/interdictions"
        android:gravity="center"
        android:visibility="invisible"/>
    <TextView
        android:id="@+id/txtNoInterdictions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/noInterdictions"
        android:gravity="center"
        android:visibility="invisible"/>
    <ListView
        android:id="@+id/lvInterdictions"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</TableLayout>